{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Environment configuration for new React apps",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "start": "webpack-dev-server --open --config webpack.config.dev.js",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .git-ignore --write \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format lint test build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/somethingvague/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "test",
    "webpack",
    "jest",
    "babel",
    "prettier"
  ],
  "author": "somethingvague",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/somethingvague/react-boilerplate/issues"
  },
  "homepage": "https://github.com/somethingvague/react-boilerplate#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@hot-loader/react-dom": "^17.0.0-rc.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "babel-loader": "^8.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-react": "^7.21.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.13.0"
  }
}
